{
  "name": "unexpected-dom",
  "version": "4.6.2",
  "description": "DOM plugin for the unexpected assertion libary",
  "main": "lib/index.js",
  "files": [
    "lib",
    "unexpected-dom.js",
    "unexpected-dom.min.js"
  ],
  "scripts": {
    "build:browserify": "browserify lib/index.js --standalone unexpected.dom --debug --x jsdom > unexpected-dom.js",
    "build:min": "uglifyjs unexpected-dom.js > unexpected-dom.min.js",
    "build": "npm run build:browserify && npm run build:min",
    "changelog": "offline-github-changelog > CHANGELOG.md",
    "lint": "eslint .",
    "test": "mocha test documentation",
    "travis": "npm run lint && npm run coverage && npm run build",
    "coverage": "nyc --reporter=lcov --reporter=text mocha --reporter dot test documentation",
    "preversion": "npm run lint && npm run test && npm run build && git add unexpected-dom.*",
    "postversion": "npm run changelog && git add CHANGELOG.md && git commit --allow-empty -m 'Update changelog' && deploy-site.sh",
    "generate-site": "generate-site --require ./bootstrap-unexpected-markdown.js",
    "update-examples": "generate-site --require ./bootstrap-unexpected-markdown.js --update-examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unexpectedjs/unexpected-dom.git"
  },
  "keywords": [
    "unexpected",
    "dom",
    "assertion",
    "library",
    "plugin"
  ],
  "author": "Peter MÃ¼ller <munter@fumle.dk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unexpectedjs/unexpected-dom/issues"
  },
  "homepage": "https://github.com/unexpectedjs/unexpected-dom",
  "devDependencies": {
    "browserify": "^14.3.0",
    "coveralls": "^2.13.1",
    "eslint": "4.19.0",
    "eslint-config-pretty-standard": "1.3.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.0.0",
    "magicpen": "^5.12.0",
    "mocha": "^3.4.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "13.0.1",
    "offline-github-changelog": "1.2.0",
    "prettier": "1.11.1",
    "sinon": "^2.3.0",
    "uglifyjs": "^2.4.11",
    "unexpected": "10.37.2",
    "unexpected-documentation-site-generator": "4.6.1",
    "unexpected-markdown": "1.7.4",
    "unexpected-sinon": "^10.7.1"
  },
  "dependencies": {
    "extend": "^3.0.1",
    "magicpen-prism": "^2.3.0"
  },
  "peerDependencies": {
    "unexpected": "^10.27.0"
  }
}
